<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>Players</title>
    </head>
    <body>
        <table>
            <tr>
                <th>Player</th>
                <th>Yds</th>
                <th>TD</th>
                <th>Lng</th>
            </tr>
            {{#each items }}
            <tr>
                <td>{{this.player}}</td>
                <td>{{this.yds}}</td>
                <td>{{this.td}}</td>
                <td>{{this.lng}}</td>
            </tr>
            {{/each}}
        </table>
    </body>
</html>